{
  "name": "@sproutsocial/seeds-packets",
  "version": "0.4.0",
  "private": true,
  "description": "Seeds: Sprout Social's design system",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sproutsocial/seeds-packets.git"
  },
  "author": "Sprout Social",
  "scripts": {
    "format:prettier": "prettier-eslint --write --eslint-ignore --print-width=120 --single-quote --no-bracket-spacing",
    "create-packet": "yo ./packets/generator-packet",
    "precommit": "lint-staged",
    "build": "lerna run build",
    "postinstall": "lerna bootstrap",
    "commitmsg": "commitlint -e $GIT_PARAMS || (echo \"\nRead about commit message formatting here: https://github.com/sproutsocial/seeds-packets#committing-changes\" && exit 1)",
    "release": "lerna publish --conventional-commits -m \"chore(release): publish\" --independent"
  },
  "lint-staged": {
    "**/*.js": [
      "format:prettier"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^6.2.0",
    "@commitlint/config-conventional": "^6.1.3",
    "@sproutsocial/seeds-utils": "*",
    "ase-util": "^1.0.1",
    "ase-utils": "^0.1.1",
    "del": "^2.2.2",
    "globby": "^6.1.0",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-insert": "^0.5.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.6.1",
    "gulp-sass": "^3.1.0",
    "gulp-util": "^3.0.8",
    "husky": "^0.14.3",
    "js-yaml": "^3.10.0",
    "lerna": "^3.4.3",
    "lint-staged": "^4.3.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.kebabcase": "^4.1.1",
    "lodash.snakecase": "^4.1.1",
    "lodash.upperfirst": "^4.3.1",
    "make-dir": "^1.1.0",
    "merge-stream": "^1.0.1",
    "mround": "^1.0.0",
    "pascal-case": "^2.0.0",
    "prettier-eslint": "^8.2.1",
    "prettier-eslint-cli": "^4.4.0",
    "theo": "^5.0.0",
    "tinycolor2": "^1.4.1",
    "yo": "^2.0.1"
  }
}
