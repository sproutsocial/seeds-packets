[
{{#each properties as |property|}}
{{#each property as |category|}}
	{{#each category as |item|}}
	{
		"category": "{{#ifEquals item.attributes.category "bambu"}}{{ item.attributes.category }}{{else}}{{ item.attributes.type }}{{/ifEquals}}",
		"sass": "{{ item.attributes.sass }}",
		"css": "{{ item.attributes.css }}",
		"javascript": "{{ item.attributes.javascript }}",
		"android": "{{ item.attributes.android }}",
		"app": "{{ item.attributes.app }}",
		"value": {
			"hex": "{{ item.value }}",
			"rgb": "rgb({{ item.attributes.rgb.r }}, {{ item.attributes.rgb.g }}, {{ item.attributes.rgb.b }})"
		}
	}{{#if @../last}}{{#unless @last}},{{/unless}}{{else}},{{/if}}
	{{/each}}
{{/each}}
{{/each}}
]
