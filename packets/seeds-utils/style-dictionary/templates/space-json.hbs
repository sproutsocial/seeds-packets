[
{{#each properties as |property|}}
{{#each property as |category|}}
	{{#each category as |item|}}
	{
		"category": "{{ item.attributes.type }}",
		"sass": "{{ item.attributes.sass }}",
		"css": "{{ item.attributes.css }}",
		"javascript": "{{ item.attributes.javascript }}",
		"app": "{{ item.attributes.app }}",
		"value": {
			"value": "{{ item.value }}",
			"rem": "{{ item.attributes.remValue }}rem"
		}
	}{{#if @../last}}{{#unless @last}},{{/unless}}{{else}},{{/if}}
	{{/each}}
{{/each}}
{{/each}}
]
